    <Grid x:Name="mainGrid">
        <Viewbox x:Name="viewBoxInstrument" HorizontalAlignment="Center" Height="50" Margin="536,278,14,0" VerticalAlignment="Top" Width="107" Child="{StaticResource ResourceKey=Instrument}" Visibility="{Binding Path=InstrumentVisibility, Mode=OneWay}" ToolTip="{Binding Path=DeviceName, Mode=OneWay}"/>
        <Rectangle HorizontalAlignment="Left" Height="45" Margin="10,80,0,0" VerticalAlignment="Top" Width="225" Fill="#3300B1EB"/>
        <Rectangle HorizontalAlignment="Left" Height="45" Margin="10,30,0,0" VerticalAlignment="Top" Width="225" Fill="#3300B1EB"/>
        <TextBlock x:Name="textBlockX" Margin="65,32,0,0" TextWrapping="Wrap" Text="{Binding XValue}" VerticalAlignment="Top" FontSize="26.667" RenderTransformOrigin="0,0" TextAlignment="Right" HorizontalAlignment="Left" Width="110" FontWeight="Bold" MaxWidth="130" MaxHeight="35"/>
        <Menu x:Name="menuTop" Height="22" VerticalAlignment="Top">
            <MenuItem x:Name="menuItemFile" Header="File">
                <MenuItem x:Name="menuItemFileNew" Header="New" Click="menuItemFileNew_Click"/>
                <Separator/>
                <MenuItem x:Name="menuItemFileLogManualAndSweep" Header="Log Manual and Sweep" IsCheckable="True" ToolTip="Log manually triggered measurements and all frequency sweep measurements"/>
                <MenuItem x:Name="menuItemFileLogAll" Header="Log Continuous" IsCheckable="True" ToolTip="Log continually triggered measurements"/>
                <MenuItem x:Name="menuItemFileLogSettings" Header="Settings" Click="menuItemFileLogSettings_Click"/>
                <Separator/>
                <MenuItem x:Name="menuItemFileClose" Header="Close" IsEnabled="{Binding IsLogging, Mode=OneWay}" Click="menuItemFileClose_Click"/>
                <Separator/>
                <MenuItem x:Name="menuItemFileExit" Header="Exit" Click="menuItemFileExit_Click"/>
            </MenuItem>
            <MenuItem x:Name="menuItemConnection" Header="Connection" MouseEnter="MenuItemConnection_MouseEnter"/>
            <MenuItem x:Name="menuItemHelp" Header="Help">
                <MenuItem x:Name="menuItemHelpAbout" Header="About" Click="menuItemHelpAbout_Click"/>
                <MenuItem x:Name="menuItemHelpHowToConnect" Header="How to connect" Click="menuItemHelpHowToConnect_Click"/>
            </MenuItem>
        </Menu>
        <Separator Height="10" Margin="0,138,0,0" VerticalAlignment="Top"/>
        <Label x:Name="labelXUnit" Content="{Binding XUnit}" HorizontalAlignment="Left" Margin="178,27,0,0" VerticalAlignment="Top" FontSize="26.667"/>
        <Label x:Name="labelXName" Content="{Binding XSymbol}" HorizontalAlignment="Left" Margin="15,27,0,0" VerticalAlignment="Top" FontSize="26.667"/>
        <TextBlock x:Name="textBlockY" Margin="65,82,0,0" TextWrapping="Wrap" Text="{Binding YValue}" VerticalAlignment="Top" FontSize="26.667" TextAlignment="Right" HorizontalAlignment="Left" Width="110" FontWeight="Bold" MaxHeight="35" MaxWidth="130"/>
        <Label x:Name="labelYName" Content="{Binding YSymbol}" HorizontalAlignment="Left" Margin="15,77,0,0" VerticalAlignment="Top" FontSize="26.667"/>
        <Label x:Name="labelYUnit" Content="{Binding YUnit}" HorizontalAlignment="Left" Margin="178,77,0,0" VerticalAlignment="Top" FontSize="26.667"/>
        <ComboBox x:Name="comboBoxMode" HorizontalAlignment="Left" Margin="320,37,0,0" VerticalAlignment="Top" Width="75" SelectedIndex="{Binding ModeIndex, Mode=OneWay}" SelectionChanged="comboBoxMode_SelectionChanged" IsEnabled="{Binding Sweep, Converter={StaticResource InverseBooleanConverter}, Mode=OneWay}" ToolTip="Device mode – equivalent circuit model"/>
        <ComboBox x:Name="comboBoxFrequency" HorizontalAlignment="Left" Margin="320,64,0,0" VerticalAlignment="Top" Width="75" SelectedIndex="{Binding FrequencyIndex, Mode=OneWay}" SelectionChanged="comboBoxFrequency_SelectionChanged" IsEnabled="{Binding Sweep, Converter={StaticResource InverseBooleanConverter}, Mode=OneWay}" ToolTip="Test signal frequency"/>
        <TextBox x:Name="textBoxVoltage" HorizontalAlignment="Left" Height="23" Margin="320,91,0,0" TextWrapping="Wrap" Text="{Binding TestSignal, Mode=OneWay}" VerticalAlignment="Top" Width="75" KeyUp="textBoxVoltage_KeyUp" IsEnabled="{Binding Sweep, Converter={StaticResource InverseBooleanConverter}, Mode=OneWay}" VerticalContentAlignment="Center" ToolTip="Test signal voltage, press Enter to assign a new value"/>
        <Label x:Name="labelVoltage" Content="Vrms" HorizontalAlignment="Left" Margin="398,89,0,0" VerticalAlignment="Top"/>
        <GroupBox x:Name="groupBoxModel" Header="Circuit model" HorizontalAlignment="Left" Margin="170,150,0,0" VerticalAlignment="Top" Height="100" Width="150" IsEnabled="{Binding Sweep, Converter={StaticResource InverseBooleanConverter}, Mode=OneWay}">
            <Grid Margin="0,5,0,0">
                <RadioButton x:Name="radioButtonModelSeries" Content="Series" HorizontalAlignment="Left" Margin="10,35,0,0" VerticalAlignment="Top" IsChecked="{Binding ModelSeries, Mode=TwoWay}" ToolTip="Switch to series component model"/>
                <RadioButton x:Name="radioButtonModelParallel" Content="Parallel" HorizontalAlignment="Left" Margin="10,55,0,0" VerticalAlignment="Top" IsChecked="{Binding ModelParallel, Mode=TwoWay}" ToolTip="Switch to parallel component model"/>
                <Button x:Name="buttonModelAuto" Content="Auto" Margin="10,5,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="75" Click="buttonModelAuto_Click" ToolTip="Set automatic circuit model"/>
            </Grid>
        </GroupBox>
        <GroupBox x:Name="groupBoxTrigger" Header="Trigger" HorizontalAlignment="Left" Margin="10,150,0,0" VerticalAlignment="Top" Height="100" Width="150" IsEnabled="{Binding Sweep, Converter={StaticResource InverseBooleanConverter}, Mode=OneWay}">
            <Grid Margin="0,5,0,0">
                <Button x:Name="buttonMeasure" Content="Measure" Margin="10,5,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="75" Click="buttonMeasure_Click" ToolTip="Trigger single measurement"/>
                <RadioButton x:Name="radioButtonMeasureSingle" Content="Single" HorizontalAlignment="Left" Margin="10,35,0,0" VerticalAlignment="Top" GroupName="Measure" IsChecked="{Binding TriggerSingle, Mode=TwoWay}" ToolTip="Switch to single measurement"/>
                <RadioButton x:Name="radioButtonMeasureContinuous" Content="Continuous" HorizontalAlignment="Left" Margin="10,55,0,0" VerticalAlignment="Top" GroupName="Measure" IsChecked="{Binding TriggerContinuous, Mode=TwoWay}" ToolTip="Switch to continuous measurement (default)"/>
            </Grid>
        </GroupBox>
        <GroupBox x:Name="groupBoxSpeed" Header="Measurement speed" HorizontalAlignment="Left" Margin="170,255,0,0" VerticalAlignment="Top" Height="100" Width="150" IsEnabled="{Binding Sweep, Converter={StaticResource InverseBooleanConverter}, Mode=OneWay}">
            <Grid Margin="0,5,0,0">
                <RadioButton x:Name="radioButtonSpeedFast" Content="Fast (14 SPS)" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" GroupName="Speed" IsChecked="{Binding SpeedFast, Mode=TwoWay}" ToolTip="Switch to fast measurement speed"/>
                <RadioButton x:Name="radioButtonSpeedMedium" Content="Medium (8 SPS)" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top" GroupName="Speed" IsChecked="{Binding SpeedMedium, Mode=TwoWay}" ToolTip="Switch to medium measurement speed"/>
                <RadioButton x:Name="radioButtonSpeedSlow" Content="Slow (1.5 SPS)" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top" GroupName="Speed" IsChecked="{Binding SpeedSlow, Mode=TwoWay}" ToolTip="Switch to slow measurement speed (default)"/>
            </Grid>
        </GroupBox>
        <GroupBox x:Name="groupBoxAveraging" Header="Averaging" HorizontalAlignment="Left" Margin="10,255,0,0" VerticalAlignment="Top" Height="100" Width="150" IsEnabled="{Binding Sweep, Converter={StaticResource InverseBooleanConverter}, Mode=OneWay}">
            <Grid Margin="0,5,0,0">
                <RadioButton x:Name="radioButtonAveragingNone" Content="None" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" GroupName="Averaging" IsChecked="{Binding AveragingNone, Mode=TwoWay}" ToolTip="No averaging (default)"/>
                <RadioButton x:Name="radioButtonAveragingSix" Content="Medium (6 samples)" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top" GroupName="Averaging" IsChecked="{Binding AveragingMedium, Mode=TwoWay}" ToolTip="Reject the highest and lowest measurement and calculate the average from the rest 4 samples"/>
            </Grid>
        </GroupBox>
        <GroupBox x:Name="groupBoxCompensation" Header="Compensation" HorizontalAlignment="Left" Margin="440,25,0,0" VerticalAlignment="Top" Height="110" Width="200" IsEnabled="{Binding Sweep, Converter={StaticResource InverseBooleanConverter}, Mode=OneWay}">
            <Grid Margin="0,5,0,0">
                <RadioButton x:Name="radioButtonCompensationThisFrequency" Content="This frequency" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" GroupName="Compensation" IsChecked="True" ToolTip="Compensate the current frequency"/>
                <RadioButton x:Name="radioButtonCompensationAllFrequencies" Content="All frequencies (~2 minutes)" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top" GroupName="Compensation" ToolTip="Compensate all 69 frequencies"/>
                <Button x:Name="buttonCompensationOpen" Content="Open" HorizontalAlignment="Left" Margin="10,55,0,0" VerticalAlignment="Top" Width="75" Click="buttonCompensationOpen_Click" ToolTip="Compensate open circuit"/>
                <Button x:Name="buttonCompensationShort" Content="Short" HorizontalAlignment="Left" Margin="100,55,0,0" VerticalAlignment="Top" Width="75" Click="buttonCompensationShort_Click" ToolTip="Compensate short circuit"/>
            </Grid>
        </GroupBox>
        <Button x:Name="buttonReset" Content="Reset" Margin="551,334,0,0" Height="20" VerticalAlignment="Top" Click="buttonReset_Click" HorizontalAlignment="Left" Width="75" ToolTip="Reset device"/>
        <GroupBox x:Name="groupBoxSweep" Header="Frequency sweep" HorizontalAlignment="Left" Margin="330,150,0,0" VerticalAlignment="Top" Height="205" Width="200">
            <Grid Margin="0,5,0,0">
                <Button x:Name="buttonSweepStartStop" Content="{Binding SweepButtonContent, Mode=OneWay}" HorizontalAlignment="Left" Margin="105,106,0,0" VerticalAlignment="Top" Width="75" Click="buttonSweepStartStop_Click"/>
                <Label x:Name="labelSweepRepeatsRemaining" Content="{Binding RemainingRepeats, Mode=OneWay}" HorizontalAlignment="Left" Margin="10,149,0,0" VerticalAlignment="Top"/>
                <Grid x:Name="gridFrequencyInner" IsEnabled="{Binding Sweep, Converter={StaticResource InverseBooleanConverter}, Mode=OneWay}">
                    <ComboBox x:Name="comboBoxSweepStart" HorizontalAlignment="Left" Margin="105,7,-80,0" VerticalAlignment="Top" Width="75" ToolTip="Initial test signal frequency"/>
                    <ComboBox x:Name="comboBoxSweepStop" HorizontalAlignment="Left" Margin="105,34,-80,0" VerticalAlignment="Top" Width="75" ToolTip="Final test signal frequency"/>
                    <Label x:Name="labelSweepStart" Content="Start frequency" HorizontalAlignment="Left" Margin="7,5,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="labelSweepStop" Content="End frequency" HorizontalAlignment="Left" Margin="7,32,0,0" VerticalAlignment="Top"/>
                    <TextBox x:Name="textBoxSweepRepeats" HorizontalAlignment="Left" Height="23" Margin="105,61,-80,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="75" Text="1" IsEnabled="{Binding IsChecked, Converter={StaticResource InverseBooleanConverter}, ElementName=checkBoxSweepRepeatsInfinite}" VerticalContentAlignment="Center" ToolTip="Number of repeats of whole sweep sequence"/>
                    <CheckBox x:Name="checkBoxSweepRepeatsInfinite" Content="Infinite" HorizontalAlignment="Left" Margin="105,88,-62,-3" VerticalAlignment="Top" ToolTip="Repeat until manually cancelled"/>
                    <Label x:Name="labelSweepRepeats" Content="Repeats" HorizontalAlignment="Left" Margin="7,59,0,0" VerticalAlignment="Top"/>
                </Grid>
            </Grid>
        </GroupBox>
        <Button x:Name="buttonFrequencyDown" HorizontalAlignment="Left" Margin="403,75,0,0" VerticalAlignment="Top" Width="14" Height="11" Click="buttonFrequencyDown_Click" IsEnabled="{Binding Sweep, Converter={StaticResource InverseBooleanConverter}, Mode=OneWay}" ToolTip="Decrease frequency">
            <TextBlock Text="−" Margin="0,-6,0,0" />
        </Button>
        <Button x:Name="buttonFrequencyUp" HorizontalAlignment="Left" Margin="403,64,0,0" VerticalAlignment="Top" Width="14" Height="11" Click="buttonFrequencyUp_Click" IsEnabled="{Binding Sweep, Converter={StaticResource InverseBooleanConverter}, Mode=OneWay}" ToolTip="Increase frequency">
            <TextBlock Text="+" Margin="0,-6,0,0" />
        </Button>
        <Label x:Name="labelMode" Content="Mode" HorizontalAlignment="Left" Margin="250,35,0,0" VerticalAlignment="Top"/>
        <Label x:Name="labelFrequency" Content="Frequency" HorizontalAlignment="Left" Margin="250,62,0,0" VerticalAlignment="Top"/>
        <Label x:Name="labelVoltage1" Content="Test signal" HorizontalAlignment="Left" Margin="250,89,0,0" VerticalAlignment="Top"/>
        <Viewbox Width="108" Margin="535,161,0,0" HorizontalAlignment="Left" Height="56" VerticalAlignment="Top" MouseUp="logo_MouseUp" ToolTip="Open GitHub repository">
            <Border BorderThickness="1" Width="Auto" Height="Auto" MouseUp="logo_MouseUp" Background="#00000000">
                <Border.Style>
                    <Style>
                        <Style.Triggers>
                            <Trigger Property="Border.IsMouseOver" Value="True">
                                <Setter Property="Border.BorderBrush" Value="#FF0466AA" />
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </Border.Style>
                <ContentControl Content="{StaticResource Logo}" Margin="10, 10, 10, 10"></ContentControl>
            </Border>
        </Viewbox>
        <StatusBar x:Name="statusBarBottom" Height="24" VerticalAlignment="Bottom">
            <StatusBarItem x:Name="statusBarBottomFilePath" HorizontalAlignment="Left" Content="{Binding FilePath}"/>
            <StatusBarItem x:Name="statusBarBottomConnection" HorizontalAlignment="Right" Content="{Binding ConnectionDetails}"/>
        </StatusBar>
        <Rectangle Fill="#00000000" HorizontalAlignment="Left" Height="95" Margin="10,30,0,0" VerticalAlignment="Top" Width="225">
            <Rectangle.ContextMenu>
                <ContextMenu>
                    <MenuItem x:Name="CopyMainValues" Click="MenuItemCopyMainValues_Click" Header="Copy values only">
                        <MenuItem.Icon>
                            <Viewbox Child="{StaticResource ClipboardValueIcon}"/>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem x:Name="CopyMainValuesWithUnits" Click="MenuItemCopyMainValuesWithUnits_Click"  Header="Copy values with units">
                        <MenuItem.Icon>
                            <Viewbox Child="{StaticResource ClipboardUnitIcon}"/>
                        </MenuItem.Icon>
                    </MenuItem>
                </ContextMenu>
            </Rectangle.ContextMenu>
        </Rectangle>
    </Grid>
</Window>
